{
  "name": "processtracker",
  "publisher": "anshul-shah",
  "displayName": "ProcessTracker",
  "description": "An extension to track the programming process",
  "icon": "images/robot.png",
  "version": "0.5.0",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Education"
  ],
  "keywords": [
    "process tracker",
    "cse190",
    "large code bases"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "processtracker.exportData",
        "title": "Export Process Data"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
